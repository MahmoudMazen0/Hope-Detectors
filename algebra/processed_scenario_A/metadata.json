        "normalization_method": "StandardScaler (Z-score)",
        "balanced": true,
        "balance_method": "SMOTE (training data only)",
        "missing_values": "None",
        "imbalance_ratio": 1.9711538461538463
    },
    "data_splits": {
        "train_samples": 615,
        "test_samples": 120,
        "test_ratio": 0.2,
        "train_distribution": {
            "1": 205,
            "2": 205,
            "3": 205
        },
        "test_distribution": {
            "1": 51,
            "2": 43,
            "3": 26
        }
    }
