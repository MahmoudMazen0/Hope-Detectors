{
    "scenario": "A",
    "description": "All patients have cancer - Predict stage (severity)",
    "dataset": {
        "total_patients": 600,
        "all_have_cancer": true
    },
    "features": {
        "names": [
            "age",
            "sex_encoded",
            "creatinine",
            "bilirubin",
            "glucose",
            "urine_volume",
            "urine_pH"
        ],
        "count": 7,
        "type": "Routine blood/urine tests only"
    },
    "target": {
        "name": "stage",
